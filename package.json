{
  "name": "union-value",
  "description": "Set an array of unique values as the property of an object. Supports setting deeply nested properties using using object-paths/dot notation.",
  "version": "2.0.1",
  "homepage": "https://github.com/jonschlinkert/union-value",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/union-value",
  "type": "module",
  "bugs": {
    "url": "https://github.com/jonschlinkert/union-value/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=12.20"
  },
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "array",
    "dot",
    "get",
    "has",
    "nested",
    "notation",
    "object",
    "path",
    "prop",
    "property",
    "set",
    "union",
    "value"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assign-value",
        "get-value",
        "has-value",
        "set-value",
        "unset-value"
      ]
    },
    "lint": {
      "reflinks": true
    }
  },
  "dependencies": {
    "get-value": "^3.0.1",
    "set-value": "^4.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^2.0.0",
    "mocha": "^9.0.3"
  }
}
